{
  "expo": {
    "name": "Menutha",
    "slug": "ft",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/menutha.png",
    "scheme": "ft",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "AIzaSyB5P-PTRn7E0xkRlkiHWkjadh3nbT7yu7U"
      },
      "bundleIdentifier": "com.menutha.customer",
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs access to the camera to scan QR codes for table orders."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/menutha.png",
        "backgroundColor": "#ffffff"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyB5P-PTRn7E0xkRlkiHWkjadh3nbT7yu7U"
        }
      },
      "edgeToEdgeEnabled": true,
      "package": "com.menutha.customer",
      "usesCleartextTraffic": true,
