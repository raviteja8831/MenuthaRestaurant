        "INTERNET",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "DEFAULT_LANDING_PAGE": "/login",
      "API_BASE_URL": "http://13.127.228.119:8090/api",
      "IMG_BASE_URL": "http://13.127.228.119:8090/"
    }
